    assign name_index = store_location_index | append: 'name'
    assign address_index = store_location_index | append: 'address'
    assign hours_weekdays = store_location_index | append: 'weekdays'
    assign hours_weekends = store_location_index | append: 'weekends'
    assign latitude_index = store_location_index | append: 'latitude'
    assign longitude_index = store_location_index | append: 'longitude'
    assign show_on_locator = store_location_index | append: 'show_on_locator'
    assign store_page_index = store_location_index | append: 'store_page'
    
    if settings[latitude_index] != blank
      assign StoreLocations = StoreLocations | append: '{"name":' | append: '"' | append: settings[name_index] | append: '",'
      assign StoreLocations = StoreLocations | append: '"address":' | append: '"' | append: settings[address_index] | append: '",'
      assign StoreLocations = StoreLocations | append: '"hours_weekdays":' | append: '"' | append: settings[hours_weekdays] | append: '",'
      assign StoreLocations = StoreLocations | append: '"hours_weekends":' | append: '"' | append: settings[hours_weekends] | append: '",'
      assign StoreLocations = StoreLocations | append: '"latitude":' | append: settings[latitude_index] | append: ','
      assign StoreLocations = StoreLocations | append: '"longitude":' | append: settings[longitude_index] | append: ','
      assign StoreLocations = StoreLocations | append: '"show_on_locator":' | append: settings[show_on_locator] | append: ','
      assign StoreLocations = StoreLocations | append: '"store_page":' | append: '"' | append: settings[store_page_index] | append: '"},'
    endif
  endfor

  assign StoreLocations = "[" | append: StoreLocations | remove_last: ',' | append: "]"
-%}

<script>
  window.StoreLocations = {{ StoreLocations | strip_html | replace: 'Sun:', '  Sun:' | json }};
</script>

